{
    "name": "ohana",
    "version": "1.0.0",
    "description": "Ohana API",
    "scripts": {
        "test": "jest --selectProjects unit",
        "test:all": "jest",
        "test:unit": "jest --selectProjects unit",
        "test:e2e": "dotenv -e .env.e2e -- jest --selectProjects e2e",
        "test:e2e:dev": "cross-env E2E_STAGE=dev dotenv -e .env.e2e -- jest --selectProjects e2e",
        "test:e2e:staging": "cross-env E2E_STAGE=staging dotenv -e .env.e2e -- jest --selectProjects e2e",
        "test:e2e:local": "cross-env E2E_STAGE=local -- jest --selectProjects e2e",
        "test:watch": "jest --watch --selectProjects unit",
        "test:coverage": "jest --coverage --selectProjects unit",
        "db:generate": "drizzle-kit generate",
        "db:seed": "serverless invoke -f runSeed",
        "docs:build": "redocly build-docs docs/ohana.json --output docs/dist/index.html",
        "docs:deploy": "serverless client deploy",
        "cleanup:users": "AWS_PROFILE=ohana-deployer node scripts/cleanup-test-users.js",
        "cleanup:users:dry": "AWS_PROFILE=ohana-deployer node scripts/cleanup-test-users.js --dry-run",
        "cleanup:users:all": "AWS_PROFILE=ohana-deployer node scripts/cleanup-test-users.js --pattern @example.com"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@aws-sdk/client-cognito-identity": "^3.787.0",
        "@aws-sdk/client-cognito-identity-provider": "^3.782.0",
        "@aws-sdk/client-rds": "^3.797.0",
        "@aws-sdk/client-s3": "^3.806.0",
        "@aws-sdk/client-sns": "^3.787.0",
        "@aws-sdk/rds-signer": "^3.797.0",
        "@aws-sdk/s3-request-presigner": "^3.806.0",
        "@redocly/cli": "^1.34.3",
        "@types/aws-lambda": "^8.10.149",
        "@types/jest": "^29.5.14",
        "@types/mime-types": "^2.1.4",
        "@types/node": "^22.14.0",
        "@types/pg": "^8.15.4",
        "axios": "^1.10.0",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^8.0.0",
        "drizzle-kit": "^0.31.0",
        "jest": "^29.7.0",
        "serverless-finch": "^4.0.4",
        "serverless-plugin-scripts": "^1.0.2",
        "ts-jest": "^29.3.3",
        "ts-node": "^10.9.2"
    },
    "dependencies": {
        "@middy/core": "^6.1.6",
        "@middy/http-cors": "^6.1.6",
        "@middy/http-header-normalizer": "^6.2.3",
        "@middy/http-json-body-parser": "^6.2.2",
        "dayjs": "^1.11.13",
        "drizzle-orm": "^0.43.1",
        "generate-password": "^1.7.1",
        "google-auth-library": "^9.15.1",
        "jwt-decode": "^4.0.0",
        "mime-types": "^3.0.1",
        "pg": "^8.15.6",
        "postgres": "^3.4.5",
        "stripe": "^18.1.0",
        "twilio": "^5.5.2",
        "uuid": "^11.1.0",
        "zod": "^3.24.3"
    }
}
